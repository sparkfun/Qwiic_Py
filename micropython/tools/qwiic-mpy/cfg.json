{
  "patterns": [
    "qwiic_i2c/qwiic_i2c/*.py",
    "qwiic/drivers/**/*.py",
    "micropython/src/__future__.py",
    "micropython/src/boards/**/*.py"
  ],
  "exclude": [
    "**/docs/conf.py",
    "**/setup.py"
  ],
  "mpy-cross": {
    "flags": [
      "-march=armv7m",
      "-mno-unicode"
    ]
  }
}
